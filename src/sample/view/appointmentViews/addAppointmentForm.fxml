<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.appointmentControllers.AddAppointmentController">
    <children>
        <VBox prefHeight="600.0" prefWidth="800.0">
            <children>
                <Label text="ADD APPOINTMENT">
                    <font>
                        <Font size="36.0" />
                    </font>
                </Label>
                <GridPane maxWidth="800.0" prefHeight="450.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="160.0" minWidth="10.0" prefWidth="75.0" />
                        <ColumnConstraints hgrow="SOMETIMES" maxWidth="230.0" minWidth="10.0" prefWidth="125.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="230.0" minWidth="10.0" prefWidth="75.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="230.0" minWidth="10.0" prefWidth="125.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="25.0" text="Title" />
                        <TextField fx:id="appTitleField" alignment="CENTER" GridPane.columnIndex="1" />
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="25.0" text="Description" GridPane.rowIndex="1" />
                        <TextField fx:id="appDescriptionField" alignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="25.0" text="Location" GridPane.rowIndex="2" />
                        <TextField fx:id="appLocationField" alignment="CENTER" prefWidth="192.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="25.0" text="Type" GridPane.rowIndex="3" />
                        <TextField fx:id="appTypeField" alignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                        <Label GridPane.rowIndex="5" />
                        <Label text="Date" GridPane.rowIndex="4" />
                  <DatePicker fx:id="appDate" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                  <Label text="Start Time" wrapText="true" GridPane.rowIndex="5" />
                  <Label layoutX="254.0" layoutY="213.0" text="End Time" wrapText="true" GridPane.columnIndex="2" GridPane.rowIndex="5" />
                  <Label text="Customer" GridPane.rowIndex="6" />
                  <Label text="User" GridPane.rowIndex="7" />
                  <Label text="Contact" GridPane.rowIndex="8" />
                  <ComboBox fx:id="appCustomerCombo" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="6" />
                  <ComboBox fx:id="appUserCombo" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="7" />
                  <ComboBox fx:id="appContactCombo" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="8" />
                  <HBox alignment="CENTER" fillHeight="false" GridPane.columnIndex="1" GridPane.rowIndex="5">
                     <children>
                        <ComboBox fx:id="appStartHourCombo" prefWidth="150.0" />
                        <ComboBox fx:id="appStartMinuteCombo" prefWidth="150.0" />
                        <ComboBox fx:id="appStartAmPmCombo" prefWidth="150.0" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="3" GridPane.rowIndex="5">
                     <children>
                        <ComboBox fx:id="appEndHourCombo" prefWidth="150.0" />
                        <ComboBox fx:id="appEndMinuteCombo" prefWidth="150.0" />
                        <ComboBox fx:id="appEndAmPmCombo" prefWidth="150.0" />
                     </children>
                  </HBox>
                    </children>
                    <VBox.margin>
                        <Insets left="15.0" />
                    </VBox.margin>
                </GridPane>
                <ButtonBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="200.0">
                    <buttons>
                        <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelButtonHandler" text="Cancel" />
                        <Button fx:id="saveButton" mnemonicParsing="false" onAction="#saveButtonHandler" text="Save" />
                    </buttons>
                </ButtonBar>
            </children>
            <padding>
                <Insets left="25.0" top="15.0" />
            </padding>
        </VBox>
    </children>
</AnchorPane>
